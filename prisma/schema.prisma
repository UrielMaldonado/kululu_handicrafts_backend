generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model ropa_artesanal {
  id          Int      @id @default(autoincrement())
  nombre      String
  descripcion String
  precio      Float
  imagen      String   
}
model accesorio_artesanal {
  id          Int      @id @default(autoincrement())
  nombre      String
  descripcion String
  precio      Float
  imagen      String   
}
